<%= form_with(model: demand, local: true) do |form| %>
  <% if demand.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(demand.errors.count, "error") %> prohibited this demand from being saved:</h2>

      <ul>
        <% demand.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :code %>
    <%= form.text_field :code %>
  </div>

  <div class="field">
    <%= form.label :region_id %>
    <%= form.number_field :region_id %>
  </div>

  <div class="field">
    <%= form.label :supervisor_id %>
    <%= form.number_field :supervisor_id %>
  </div>

  <div class="field">
    <%= form.label :owner %>
    <%= form.text_field :owner %>
  </div>

  <div class="field">
    <%= form.label :city %>
    <%= form.text_field :city %>
  </div>

  <div class="field">
    <%= form.label :demandType %>
    <%= form.text_field :demandType %>
  </div>

  <div class="field">
    <%= form.label :dueDate %>
    <%= form.date_select :dueDate %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
